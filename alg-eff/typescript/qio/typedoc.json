{
  "excludeExternals": false,
  "excludeNotExported": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "hideGenerator": true,
  "mode": "file",
  "out": "packages/docs/api",
  "exclude": ["test/**", "example/**", "benchmarks/**"],
  "plugin": ["typedoc-plugin-markdown"],
  "listInvalidSymbolLinks": true,
  "toc": [
    "QIO",
    "UIO",
    "IO",
    "Task",
    "TaskR",
    "Ref",
    "Fiber",
    "Await",
    "Exit",
    "Managed",
    "Queue",
    "Stream",
    "IRuntime",
    "BaseRuntime",
    "DefaultRuntime",
    "TestRuntime"
  ]
}
